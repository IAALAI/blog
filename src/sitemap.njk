---
permalink: sitemap.xml
noindex: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for item in collections.all %}
{% if not item.data.noindex %}
    <url>
        <loc>{{site.full_host}}{{item.page.url}}</loc>
        <lastmod>{{ (item.data.last or build.date).toISOString() }}</lastmod>
    </url>
{% endif %}
{% endfor %}
</urlset>